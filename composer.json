{
    "name": "santran/huobi",
    "description": "PHP Huobi.Pro API is an asynchronous PHP library for the Huobi API designed to be easy to use.",
    "keywords": ["Huobi.Pro","cryptocurrency","WebSocket","trading","api","laravel"],
    "license": "MIT",
    "repositories": [ {
            "type": "vcs",
            "url": "https://github.com/santran/huobi"
    }],
    "require": {
        "php": ">=5.4",
        "ratchet/pawl": "^0.3.0",
        "react/socket": "^1.0 || ^0.8 || ^0.7",
        "ratchet/rfc6455": "^0.2.3",
        "santran/huobi": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8"
    },
    "config": {
    	"optimize-autoloader": true,
        "minimum-stability": "dev"
    },
    "autoload": {
        "classmap": [
          "huobi.php"
        ]
    }
}
